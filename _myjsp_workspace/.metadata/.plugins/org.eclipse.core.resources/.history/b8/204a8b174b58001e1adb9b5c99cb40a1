package domain;

public class MemverVO {
	private String id;
	private String pw;
	private String name;
	private int age;
	private String phone;
	private String regDate;
	private String lastLogin;

	// 로그인용
	public MemverVO(String id, String pw) {
		this.id = id;
		this.pw = pw;
	}

	// 회원가입용
	public MemverVO(String id, String pw, String name, int age, String phone) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.age = age;
		this.phone = phone;
	}

	// 회원 리스트
	public MemverVO(String id, String name, int age) {
		this.id = id;
		this.name = name;
		this.age = age;
	}

	// 회원 상세정보
	public MemverVO(String id, String pw, String name, int age, String phone, String regDate, String lastLogin) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.age = age;
		this.phone = phone;
		this.regDate = regDate;
		this.lastLogin = lastLogin;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPw() {
		return pw;
	}

	public void setPw(String pw) {
		this.pw = pw;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getRegDate() {
		return regDate;
	}

	public void setRegDate(String regDate) {
		this.regDate = regDate;
	}

	public String getLastLogin() {
		return lastLogin;
	}

	public void setLastLogin(String lastLogin) {
		this.lastLogin = lastLogin;
	}

	@Override
	public String toString() {
		return id + "pw=" + pw + ", name=" + name + ", age=" + age + ", phone=" + phone
				+ ", regDate=" + regDate + ", lastLogin=" + lastLogin ;
	}

}
